<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <corners
        android:radius="50dp"
        />
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
            <size
                android:width="10dp"
                android:height="10dp"/>
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <stroke android:color="@android:color/transparent"
                android:width="5dp"/>
            <solid android:color="@color/back_no_check_count"/>
            <size
                android:width="10dp"
                android:height="10dp"/>
        </shape>
    </item>
</layer-list>